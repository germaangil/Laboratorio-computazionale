    RowBox[{"Total", "@", 
     RowBox[{"IntegerDigits", "[", "n", "]"}]}]}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Concatena", " ", "m", " ", "con", " ", "opPrimi", " ", "del", " ", 
