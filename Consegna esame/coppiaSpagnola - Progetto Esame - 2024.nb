Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Funzione", " ", "per", " ", "riflettere", " ", "un", " ", "punto", " ", 
    "rispetto", " ", "ad", " ", "una", " ", "geodetica"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"rifletta", "[", 
     RowBox[{"P_", ",", "P1_", ",", "P2_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "z0", ",", "z1", ",", "z2", ",", "z", ",", "\[Alpha]", ",", "c", ",", 
        "eq1", ",", "eq2", ",", "inters"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"z1", "=", 
        RowBox[{
         RowBox[{"P1", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "+", 
         RowBox[{
          RowBox[{"P1", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "*", "I"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"z2", "=", 
        RowBox[{
         RowBox[{"P2", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "+", 
         RowBox[{
          RowBox[{"P2", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "*", "I"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"z0", "=", 
        RowBox[{
         RowBox[{"P", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "+", 
         RowBox[{
          RowBox[{"P", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "*", "I"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Det", "[", 
           RowBox[{"{", 
            RowBox[{"P1", ",", "P2"}], "}"}], "]"}], "==", "0"}], ",", 
         "\[IndentingNewLine]", "\t", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"P1", "!=", 
             RowBox[{"{", 
              RowBox[{"0", ",", "0"}], "}"}]}], ",", " ", 
            RowBox[{"eq1", "=", 
             RowBox[{
              RowBox[{
               RowBox[{"P1", "[", 
                RowBox[{"[", "2", "]"}], "]"}], "*", "x"}], "==", 
              RowBox[{
               RowBox[{"P1", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "*", "y"}]}]}], ",", " ", 
            RowBox[{"eq1", "=", 
             RowBox[{
              RowBox[{
               RowBox[{"P2", "[", 
                RowBox[{"[", "2", "]"}], "]"}], "*", "x"}], "==", 
              RowBox[{
               RowBox[{"P2", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "*", "y"}]}]}]}], "]"}], ";", 
          "\[IndentingNewLine]", "\t", 
          RowBox[{"eq2", "=", 
           RowBox[{
            RowBox[{
             SuperscriptBox["x", "2"], "+", 
             SuperscriptBox["y", "2"]}], "==", "1"}]}], ";", 
          "\[IndentingNewLine]", "\t", 
          RowBox[{"inters", "=", 
           RowBox[{"NSolve", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"eq1", ",", "eq2"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "y"}], "}"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", "\t", 
          RowBox[{"\[Alpha]", "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"x", "/.", 
              RowBox[{"inters", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], ")"}], "+", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"y", "/.", 
               RowBox[{"inters", "[", 
                RowBox[{"[", "1", "]"}], "]"}]}], ")"}], "*", "I"}]}]}], ";", 
          "\[IndentingNewLine]", "\t", 
          RowBox[{"z", "=", 
           RowBox[{
            RowBox[{
             SuperscriptBox["\[Alpha]", "2"], "*", 
             RowBox[{"Conjugate", "[", "z0", "]"}]}], "//", "N"}]}], ";"}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"(*", "else", "*)"}], "\[IndentingNewLine]", "\t", 
         RowBox[{
          RowBox[{"c", "=", 
           RowBox[{"centro", "[", 
            RowBox[{"P1", ",", "P2"}], "]"}]}], ";", "\[IndentingNewLine]", 
          "\t", 
          RowBox[{"\[Alpha]", "=", 
           RowBox[{
            RowBox[{"c", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "+", 
            RowBox[{
             RowBox[{"c", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "*", "I"}]}]}], ";", "\t", 
          "\[IndentingNewLine]", "\t", 
          RowBox[{"z", "=", 
           FormBox[
            RowBox[{
             FractionBox[
              RowBox[{
               RowBox[{"\[Alpha]", "*", 
                RowBox[{"Conjugate", "[", "z0", "]"}]}], "-", "1"}], 
              RowBox[{
               RowBox[{"Conjugate", "[", "z0", "]"}], "-", 
               RowBox[{"Conjugate", "[", "\[Alpha]", "]"}]}]], "//", "N"}],
            TraditionalForm]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"P1", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Re", "[", "z", "]"}], ",", 
           RowBox[{"Im", "[", "z", "]"}]}], "}"}]}], "}"}]}]}], "]"}]}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Rifletta", " ", "un", " ", "punto", " ", "rispetto", " ", "ad", " ", 
     "un", " ", "insieme", " ", "di", " ", "geodetiche"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"riflettaGeodetiche", "[", 
     RowBox[{"P_", ",", "punti_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"nuoviPunti", ",", " ", "N", ",", "riflessione"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"nuoviPunti", "=", 
        RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"N", "=", 
        RowBox[{"Length", "[", "punti", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"riflessione", "=", 
           RowBox[{"rifletta", "[", 
            RowBox[{"P", ",", 
             RowBox[{"punti", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", 
             RowBox[{"punti", "[", 
              RowBox[{"[", 
               RowBox[{"i", "+", "1"}], "]"}], "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"nuoviPunti", "=", 
           RowBox[{"Join", "[", 
            RowBox[{"nuoviPunti", ",", " ", "riflessione"}], "]"}]}], ";"}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", 
