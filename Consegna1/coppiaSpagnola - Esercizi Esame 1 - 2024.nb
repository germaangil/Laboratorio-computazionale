Cell["Indicazioni :", "Subsubsection",
 CellChangeTimes->{{3.667325283819504*^9, 3.667325298109525*^9}, 
   3.667325435839716*^9},
 FontSize->14,ExpressionUUID->"d34ad650-d709-42f8-aec8-adb7fba38c41"],

Cell[TextData[{
 "Farlo per cinque o sei valori di ",
 StyleBox["n",
  FontSlant->"Italic"],
 " compresi fra ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["10", "2"], TraditionalForm]],ExpressionUUID->
  "6257e42e-c299-4bf7-bf14-92b61ac49034"],
 " e ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["10", "8"], TraditionalForm]],ExpressionUUID->
  "94e6692c-5fb8-4c38-a26d-d0f1e1ad717f"],
 " o ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["10", "9"], TraditionalForm]],ExpressionUUID->
  "1010f008-d930-4480-a325-efaca41c2b59"],
 ".\nScegliere ",
 StyleBox["\[CapitalDelta]n",
  FontSlant->"Italic"],
 " in funzione di ",
 StyleBox["n",
  FontSlant->"Italic"],
 ", in modo che il numero ",
 StyleBox["k",
  FontSlant->"Italic"],
 " di numeri primi nell\[CloseCurlyQuote]intervallo (",
 StyleBox["n, n+\[CapitalDelta]n",
  FontSlant->"Italic"],
 "), e dunque il numero di \[OpenCurlyDoubleQuote]scalini\
\[CloseCurlyDoubleQuote], sia (circa) indipendente da ",
 StyleBox["n",
  FontSlant->"Italic"],
 ".\nPer facilitare il confronto finale, produrre una sola figura con tutti i \
grafici."
}], "Text",
 CellChangeTimes->{{3.667326711321504*^9, 3.667326749001558*^9}, {
  3.6673291235348787`*^9, 3.667329125894882*^9}, {3.66732918803497*^9, 
