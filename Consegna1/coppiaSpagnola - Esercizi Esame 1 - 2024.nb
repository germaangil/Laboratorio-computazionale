Cell[TextData[{
 "Cercare di produrre grafici di ",
 StyleBox["\[Pi](x)",
  FontSlant->"Italic"],
 ",  in diversi intervalli ",
 StyleBox["n \[LessEqual] x \[LessEqual] n+\[CapitalDelta]n",
  FontSlant->"Italic"],
 ", in modo che si assomiglino."
}], "Text",
 CellFrame->True,
 CellChangeTimes->{{3.667326332560972*^9, 3.667326372321028*^9}, 
   3.667326719941514*^9},
