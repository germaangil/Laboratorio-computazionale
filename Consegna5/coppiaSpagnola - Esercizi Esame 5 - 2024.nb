Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Definizione", " ", "del", " ", "sistema", " ", "di", " ", "Lorenz"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"X", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"x_", ",", "y_", ",", "z_"}], "}"}], ",", "\[Sigma]_", ",", 
       "\[Beta]_", ",", "r_"}], "]"}], ":=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\[Sigma]", " ", 
        RowBox[{"(", 
         RowBox[{"y", "-", "x"}], ")"}]}], ",", 
       RowBox[{
        RowBox[{"x", " ", 
         RowBox[{"(", 
          RowBox[{"r", "-", "z"}], ")"}]}], "-", "y"}], ",", 
       RowBox[{
        RowBox[{"x", " ", "y"}], "-", 
        RowBox[{"\[Beta]", " ", "z"}]}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Metodo", " ", "di", " ", "Runge"}], "-", 
     RowBox[{"Kutta", " ", "di", " ", "quarto", " ", "ordine"}]}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"Intstep4", "[", 
       RowBox[{"h_", ",", "\[Sigma]_", ",", "\[Beta]_", ",", "r_"}], "]"}], 
      "[", "z_", "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"v1", ",", "v2", ",", "v3", ",", "v4"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"v1", "=", 
         RowBox[{"h", " ", 
          RowBox[{"X", "[", 
           RowBox[{"z", ",", "\[Sigma]", ",", "\[Beta]", ",", "r"}], 
           "]"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"v2", "=", 
         RowBox[{"h", " ", 
          RowBox[{"X", "[", 
           RowBox[{
            RowBox[{"z", "+", 
             FractionBox["v1", "2"]}], ",", "\[Sigma]", ",", "\[Beta]", ",", 
            "r"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"v3", "=", 
         RowBox[{"h", " ", 
          RowBox[{"X", "[", 
           RowBox[{
            RowBox[{"z", "+", 
             FractionBox["v2", "2"]}], ",", "\[Sigma]", ",", "\[Beta]", ",", 
            "r"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"v4", "=", 
         RowBox[{"h", " ", 
          RowBox[{"X", "[", 
           RowBox[{
            RowBox[{"z", "+", "v3"}], ",", "\[Sigma]", ",", "\[Beta]", ",", 
            "r"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"z", " ", "+", " ", 
         RowBox[{
          FractionBox["1", "6"], 
          RowBox[{"(", 
           RowBox[{"v1", "+", 
            RowBox[{"2", " ", "v2"}], "+", 
            RowBox[{"2", " ", "v3"}], "+", "v4"}], ")"}]}]}]}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Integratore4", "[", 
      RowBox[{
      "z_", ",", "tmax_", ",", "h_", ",", "\[Sigma]_", ",", "\[Beta]_", ",", 
       "r_"}], "]"}], ":=", 
     RowBox[{"NestList", "[", 
      RowBox[{
       RowBox[{"Intstep4", "[", 
        RowBox[{"h", ",", "\[Sigma]", ",", "\[Beta]", ",", "r"}], "]"}], ",", 
       "z", ",", 
       RowBox[{"Abs", "[", 
        RowBox[{"Floor", "[", 
         RowBox[{"tmax", "/", "h"}], "]"}], "]"}]}], "]"}]}], 
    ";"}]}]}]], "Input",
 CellChangeTimes->{{3.926165108484138*^9, 3.926165192823824*^9}, {
